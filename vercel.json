{
  "version": 2,
  "builds": [
    {
      "src": "src/workflow_project/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.13",
        "maxLambdaSize": "250mb",
        "includeFiles": "src/**",
        "excludeFiles": "{.git/**,node_modules/**,*.pyc,__pycache__/**,tests/**,*.md,workflow_env/**}"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/workflow_project/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/vercel/path0/src",
    "VERCEL": "1"
  }
}